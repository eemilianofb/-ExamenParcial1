#include <stdio.h>

//Programa que obtiene la tabla de multiplicar de un n√∫mero.

int main(){
int n;
int numeros = 0;

	printf("Que tabla de \033[32mmultiplicar\033[0m quieres?\n",&n);
	scanf("%d", &n);


	printf("Aqui esta tu tabla de 033[32mmultiplicar\033[0m \n");
	while (numeros <= 10) {
		
	printf("%d x %d = %d\n", n, numeros, n * numeros);
        numeros++; 
    }
	
	return 0;
}
